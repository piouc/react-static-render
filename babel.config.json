{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-react",
    "@babel/preset-env"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "displayName": true,
        "fileName": true,
        "ssr": true,
        "pure": false,
        "minify": false,
        "transpileTemplateLiterals": false,
        "meaninglessFileNames": ["index"]
      }
    ]
  ],
  "sourceType": "unambiguous"
}